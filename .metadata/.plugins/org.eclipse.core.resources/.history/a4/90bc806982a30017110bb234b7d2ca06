package com.revature.corejava;
import java.util.ArrayList;

public class Q9_Primes {
	
	public static void main (String[] args) {
		arr(100);
		
	}
	
	public static ArrayList<Integer> arr(int size){
		ArrayList<Integer> nums = new ArrayList<Integer>(size);
		for (int i = 1; i<size+1; i++) {
			nums.add(i);
		}
		
		ArrayList<Integer> primes = nums;
		
		for (int n : nums) { //checks through every number in
			nums.removeIf( for (int j=1; j<n;j++) { n%j==0 });
		}
		System.out.println(primes);
		return nums;
	}
}
