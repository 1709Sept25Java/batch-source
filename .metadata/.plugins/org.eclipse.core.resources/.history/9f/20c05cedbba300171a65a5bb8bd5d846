package com.revature.corejava;

import java.util.ArrayList;

public class Q19_SumsPrimes {
	
	
	public static void main (String[] args) {
		arr = Array(10);
		sumEvens(arr);
		sumOdds(arr);
		printPrimes(arr);	
	}
	
	
	public ArrayList<Integer> Array(int n){
		ArrayList<Integer> nums = new ArrayList<Integer>();
		for (int i = 0; i<n; i++) {
			nums.add(i);
		}
		return nums;
	}
	
	public int sumEvens(ArrayList<Integer> nums) {
		int sum = 0;
		for(int n : nums) {
			if (n%2 == 0) {
				sum = sum+n;
			}
		return sum;
	}
	

}
